{
    "manifest_version": 3,
    "name": "Code Study Extension",
    "version": "1.0",
    "description": "An extension to capture, extract, and edit code from images.",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "icons": {
      "48":  "images/icon-developer-icon_48.png",
      "128": "images/icon-developer-icon_128.png"
    },
    "background": {
      "service_worker": "background.js",
      "type"          : "module"
    },
    "permissions": [
        "storage"
    ]
 
  }